{
  "name": "react-app",
  "version": "0.1.0",
  "author": "Design System Team",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "es-dev-server": "^2.1.0",
    "history": "^5.0.0",
    "lit": "^2.2.8",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false react-scripts start",
    "build": "react-scripts build -- --stats",
    "stats": "node ../../scripts/stats react",
    "serve": "es-dev-server --watch --node-resolve --open --app-index build/index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "webpack-bundle-analyzer -p 8082 build/bundle-stats.json",
    "clean:nm": "rimraf -rf ./node_modules"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@lit-labs/react": "1.0.8",
    "@types/react-router-dom": "^5.1.7",
    "rimraf": "^3.0.2",
    "webpack-bundle-analyzer": "^4.4.0"
  }
}
